depends(u2,x1,u2,x2,u2,x3,u2,x4);
depends(u3,x1,u3,x2,u3,x3,u3,x4);
depends(u4,x1,u4,x2,u4,x3,u4,x4);
depends(p,x1);
B4: diff(p,x1,4)+4*u2*diff(p,x1,2)+(4*u3+6*diff(u2,x1))*diff(p,x1)+4*u4*p+6*diff(u3,x1)*p+4*diff(u2,x1,2)*p+6*u2*u2*p;
B3: diff(p,x1,3)+3*u2*diff(p,x1)+3*u3*p+3*diff(u2,x1)*p;
A: diff(B3,x4);
B: diff(B4,x3);
W: subst(p=B3,B4);
Z: subst(p=B4,B3);
R: B-A+W-Z;
